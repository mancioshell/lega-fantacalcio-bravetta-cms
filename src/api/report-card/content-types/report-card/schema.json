{
  "kind": "collectionType",
  "collectionName": "report_cards",
  "info": {
    "singularName": "report-card",
    "pluralName": "report-cards",
    "displayName": "ReportCard",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "team": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::team.team"
    },
    "introduction": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "richtext",
      "required": true
    },
    "league_day": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "integer",
      "required": true
    },
    "report_picture": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "top_team": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::team.team"
    },
    "flop_team": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::team.team"
    },
    "matches": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::match.match"
    }
  }
}
