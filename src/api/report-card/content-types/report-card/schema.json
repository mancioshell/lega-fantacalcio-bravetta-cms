{
  "kind": "collectionType",
  "collectionName": "report_cards",
  "info": {
    "singularName": "report-card",
    "pluralName": "report-cards",
    "displayName": "Report",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "introduction": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "richtext",
      "required": true
    },
    "league_day": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "integer",
      "required": true
    },
    "report_picture": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "top_team": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::team.team"
    },
    "flop_team": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::team.team"
    },
    "matches": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "report.matches",
      "required": true,
      "min": 4,
      "max": 4
    }
  }
}
